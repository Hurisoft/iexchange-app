{
  "name": "iexchange-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:watch": "npx concurrently \"npm run codegen:get-schema:watch\"  \"npm run dev\"",
    "codegen": "npx graphql-codegen --config codegen.ts",
    "codegen:watch": "npx graphql-codegen --config codegen.ts --watch",
    "get-schema": "node get-schema.js > schema.graphql",
    "codegen:get-schema": "npm run get-schema && npm run codegen",
    "codegen:get-schema:watch": "npm run get-schema && npm run codegen:watch"
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@rainbow-me/rainbowkit": "^2.1.2",
    "@sumsub/websdk-react": "^2.3.0",
    "@tanstack/react-query": "^5.45.1",
    "@tanstack/react-query-devtools": "^5.45.1",
    "@zkpass/transgate-js-sdk": "^0.2.0",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "crypto": "^1.0.1",
    "ethers": "^6.13.1",
    "form-data": "^4.0.0",
    "lucide-react": "^0.395.0",
    "next": "14.2.4",
    "react": "^18",
    "react-data-table-component": "^7.6.2",
    "react-dom": "^18",
    "react-hook-form": "^7.52.0",
    "react-json-pretty": "^2.2.0",
    "sonner": "^1.5.0",
    "styled-components": "^6.1.11",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "husky": "^9.0.11",
    "postcss": "^8",
    "tailwindcss": "^3.4.4",
    "typescript": "^5"
  }
}
